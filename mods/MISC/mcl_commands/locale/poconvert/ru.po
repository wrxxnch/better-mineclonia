msgid "List bans"
msgstr "Список банов"

msgid "Ban list: @1"
msgstr "Список банов: @1"

msgid "Players can't be killed right now, damage has been disabled."
msgstr "Игроки не могут быть убиты прямо сейчас, урон отключён."

msgid "Player @1 does not exist."
msgstr "Игрока @1 не существует."

msgid "You are already dead"
msgstr "Вы уже мертвы"

msgid "@1 is already dead"
msgstr "@1 уже мертв(а)"

msgid "[<name>]"
msgstr "[<имя>]"

msgid "Kill player or yourself"
msgstr "Убить игрока или себя"

msgid "Show who is logged on"
msgstr "Показывает игроков в сети"

msgid "Can use /say"
msgstr "Можно использовать /say"

msgid "<message>"
msgstr "<сообщение>"

msgid "Send a message to every player"
msgstr "Отправляет сообщение всем игрокам"

msgid "Invalid usage, see /help say."
msgstr "Недопустимое использование, см. /help say."

msgid "Displays the world seed"
msgstr "Показать значение зерна мира"

msgid "<X>,<Y>,<Z> <NodeString>"
msgstr "<X>,<Y>,<Z> <ИмяБлока>"

msgid "Set node at given position"
msgstr "Устанавливает блок в заданном месте"

msgid "Invalid node"
msgstr "Неправильный блок"

msgid "@1 spawned."
msgstr "@1 возродился(ась)."

msgid "Invalid parameters (see /help setblock)"
msgstr "Недопустимые параметры (см. /help setblock)"

msgid "<sound> <target>"
msgstr "<звук> <цель>"

msgid "Play a sound. Arguments: <sound>: name of the sound. <target>: Target."
msgstr "Проигрывает звук. Аргументы: <звук>: имя звука. <цель>: Цель."

msgid "Sound name is invalid!"
msgstr "Неверное имя звука!"

msgid "Target is invalid!!"
msgstr "Цель указана неверно!"

msgid "Only peaceful mobs allowed!"
msgstr "Разрешены только мирные сущности!"

