msgid "@1 went up in flames"
msgstr "@1 сгорел(а) в языках пламени"

msgid "@1 walked into fire whilst fighting @2"
msgstr "@1 прошёлся(лась) по огню, сражаясь с @2"

msgid "@1 was struck by lightning"
msgstr "@1 убит(а) молнией"

msgid "@1 was struck by lightning whilst fighting @2"
msgstr "@1 убит(а) молнией, сражаясь с @2"

msgid "@1 burned to death"
msgstr "@1 сгорел(а) заживо"

msgid "@1 was burnt to a crisp whilst fighting @2"
msgstr "@1 обгорел(а) до углей, сражаясь с @2"

msgid "@1 tried to swim in lava"
msgstr "@1 попытался(ась) поплавать в лаве"

msgid "@1 tried to swim in lava to escape @2"
msgstr "@1 попытался(ась) переплыть лаву, убегая от @2"

msgid "@1 discovered the floor was lava"
msgstr "@1 узнал(а) что пол это лава"

msgid "@1 walked into danger zone due to @2"
msgstr "@1 прогулялся(лась) в опасной зоне, благодаря @2"

msgid "@1 suffocated in a wall"
msgstr "@1 задохнулся(ась) в стене"

msgid "@1 suffocated in a wall whilst fighting @2"
msgstr "@1 задохнулся(ась) в стене, сражаясь с @2"

msgid "@1 drowned"
msgstr "@1 утонул(а)"

msgid "@1 drowned whilst trying to escape @2"
msgstr "@1 утонул(а), убегая от @2"

msgid "@1 starved to death"
msgstr "@1 умер(ла) от голода"

msgid "@1 starved to death whilst fighting @2"
msgstr "@1 умер(ла) от голода, сражаясь с @2"

msgid "@1 was pricked to death"
msgstr "@1 заколот(а) до смерти"

msgid "@1 walked into a cactus whilst trying to escape @2"
msgstr "@1 задел(а) кактус, убегая от @2"

msgid "@1 hit the ground too hard"
msgstr "@1 слишком сильно ударился(ась) об землю"

msgid "@1 hit the ground too hard whilst trying to escape @2"
msgstr "@1 слишком сильно ударился(ась) об землю, убегая от @2"

msgid "@1 experienced kinetic energy"
msgstr "@1 испытал(а) на себе кинетическую энергию"

msgid "@1 experienced kinetic energy whilst trying to escape @2"
msgstr "@1 испытал(а) на себе кинетическую энергию, убегая от @2"

msgid "@1 fell out of the world"
msgstr "@1 выпал(а) из мира"

msgid "@1 didn't want to live in the same world as @2"
msgstr "@1 не захотел(а) жить в том же мире, что и @2"

msgid "@1 died"
msgstr "@1 погиб(ла)"

msgid "@1 died because of @2"
msgstr "@1 погиб(ла) из-за @2"

msgid "@1 was killed by magic"
msgstr "@1 убит(а) магией"

msgid "@1 was killed by magic whilst trying to escape @2"
msgstr "@1 убит(а) магией, убегая от @2"

msgid "@1 was killed by @2 using magic"
msgstr "@1 убит(а) @2 с помощью магии"

msgid "@1 was killed by @2 using @3"
msgstr "@1 убит(а) @2 с помощью @3"

msgid "@1 was roasted in dragon breath"
msgstr "@1 поджарился(ась) в драконьем дыхании"

msgid "@1 was roasted in dragon breath by @2"
msgstr "@1 поджарился(ась) в драконьем дыхании, благодаря @2"

msgid "@1 withered away"
msgstr "@1 иссох(ла)"

msgid "@1 withered away whilst fighting @2"
msgstr "@1 иссох(ла), сражаясь с @2"

msgid "@1 was shot by a skull from @2"
msgstr "@1 застрелен(а) черепом от @2"

msgid "@1 was squashed by a falling anvil"
msgstr "@1 раздавлен(а) падающей наковальней"

msgid "@1 was squashed by a falling anvil whilst fighting @2"
msgstr "@1 раздавлен(а) падающей наковальней, сражаясь с @2"

msgid "@1 was squashed by a falling block"
msgstr "@1 раздавлен(а) падающим блоком"

msgid "@1 was squashed by a falling block whilst fighting @2"
msgstr "@1 раздавлен(а) падающим блоком, сражаясь с @2"

msgid "@1 was slain by @2"
msgstr "@1 погиб(ла) от @2"

msgid "@1 was slain by @2 using @3"
msgstr "@2 убил(а) @1 с помощью своего @3"

msgid "@1 was shot by @2"
msgstr "@1 застрелен(а) @2"

msgid "@1 was shot by @2 using @3"
msgstr "@2 застрелил(а) @1 с помощью своего @3"

msgid "@1 was spitballed by @2"
msgstr "@1 заплёван(а) @2"

msgid "@1 was spitballed by @2 using @3"
msgstr "@1 заплёван(а) @2 из @3"

msgid "@1 was fireballed by @2"
msgstr "@1 получил(а) огненным снарядом от @2"

msgid "@1 was fireballed by @2 using @3"
msgstr "@1 получил(а) огненным снарядом от @2 из @3"

msgid "@1 was killed trying to hurt @2"
msgstr "@1 погиб(ла), пытаясь навредить @2"

msgid "@1 tried to hurt @2 and died by @3"
msgstr "@1 убит(а) @3, пытаясь навредить @2"

msgid "@1 blew up"
msgstr "@1 взорвался(ась)"

msgid "@1 was blown up by @2"
msgstr "@1 взорван(а) @2"

msgid "@1 was blown up by @2 using @3"
msgstr "@1 взорван(а) @2 с помощью @3"

msgid "@1 was squished too much"
msgstr "@1 сдавлен(а) в лепёшку"

msgid "@1 was squashed by @2"
msgstr "@1 сдавлен(а) в лепёшку, благодаря @2"

msgid "@1 went off with a bang"
msgstr "@1 попал(а) в мир иной под звуки салюта"

msgid "@1 went off with a bang due to a firework fired by @2 from @3"
msgstr "@1 попал(а) в мир иной под звуки салюта, выпущенного из @3 игроком @2"

msgid "@1 was poked to death by a sweet berry bush"
msgstr "@1 был до смерти заколот кустом сладких ягод"

msgid "@1 was poked to death by a sweet berry bush whilst trying to escape @2"
msgstr "@1 заколот(а) до смерти кустом сладкой ягоды, пытаясь спастись от @2"

