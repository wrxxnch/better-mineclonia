msgid "@1 went up in flames"
msgstr "@1 a péri dans les flammes"

msgid "@1 walked into fire whilst fighting @2"
msgstr "@1 a marché dans le feu en tentant de combattre (un(e)) @2"

msgid "@1 was struck by lightning"
msgstr "@1 s'est fait frapper par la foudre"

msgid "@1 was struck by lightning whilst fighting @2"
msgstr "@1 s'est fait frapper par la foudre en combattant @2"

msgid "@1 burned to death"
msgstr "@1 s'est fait carboniser à mort"

msgid "@1 was burnt to a crisp whilst fighting @2"
msgstr "@1 s'est fait réduire en cendre en tentant de combattre (un(e)) @2"

msgid "@1 tried to swim in lava"
msgstr "@1 a essayé de nager dans la lave"

msgid "@1 tried to swim in lava to escape @2"
msgstr "@1 a essayé de nager dans la lave pour fuir (un(e)) @2"

msgid "@1 discovered the floor was lava"
msgstr "@1 a découvert que le sol était fait de lave"

msgid "@1 walked into danger zone due to @2"
msgstr "@1 a marché dans une zone dangereuse à cause de/d'un(e) @2"

msgid "@1 suffocated in a wall"
msgstr "@1 a suffoqué dans un mur"

msgid "@1 suffocated in a wall whilst fighting @2"
msgstr "@1 a étouffé dans un mur en combattant @2"

msgid "@1 drowned"
msgstr "@1 s'est noyé(e)"

msgid "@1 drowned whilst trying to escape @2"
msgstr "@1 s'est noyé(e) en tentant de fuir (un(e)) @2"

msgid "@1 starved to death"
msgstr "@1 est mort(e) de faim"

msgid "@1 starved to death whilst fighting @2"
msgstr "@1 est mort(e) de faim en tentant de combattre (un(e)) @2"

msgid "@1 was pricked to death"
msgstr "@1 s'est fait piquer à mort par un cactus"

msgid "@1 walked into a cactus whilst trying to escape @2"
msgstr "@1 a foncé dans un cactus en tentant de fuir (un(e)) @2"

msgid "@1 hit the ground too hard"
msgstr "@1 s'est écrasé(e) au sol"

msgid "@1 hit the ground too hard whilst trying to escape @2"
msgstr "@1 s'est écrasé(e) au sol en tentant de fuir (un(e)) @2"

msgid "@1 experienced kinetic energy"
msgstr "@1 a découvert l'énergie cinétique"

msgid "@1 experienced kinetic energy whilst trying to escape @2"
msgstr "@1 a découvert l'énergie cinétique en tentant de fuir (un(e)) @2"

msgid "@1 fell out of the world"
msgstr "@1 a littéralement quitté notre monde"

msgid "@1 didn't want to live in the same world as @2"
msgstr "@1 n'avait pas envie de vivre dans le même monde que @2"

msgid "@1 died"
msgstr "@1 est mort(e)"

msgid "@1 died because of @2"
msgstr "@1 est mort(e) à cause de @2"

msgid "@1 was killed by magic"
msgstr "@1 a été tué(e) par magie"

msgid "@1 was killed by magic whilst trying to escape @2"
msgstr "@1 a été tué(e) par magie en tentant d'échapper à @2"

msgid "@1 was killed by @2 using magic"
msgstr "@1 s'est fait tuer par (un(e)) @2 avec un sort"

msgid "@1 was killed by @2 using @3"
msgstr "@1 a été tué(e) par @2 en utilisant @3"

msgid "@1 was roasted in dragon breath"
msgstr "@1 a été rôti(e) dans le souffle du dragon"

msgid "@1 was roasted in dragon breath by @2"
msgstr "@1 a été rôti(e) dans le souffle du dragon par @2"

msgid "@1 withered away"
msgstr "@1 est mort(e) de décomposition"

msgid "@1 withered away whilst fighting @2"
msgstr "@1 est mort(e) de décomposition en tentant de combattre (un(e)) @2"

msgid "@1 was shot by a skull from @2"
msgstr "@1 s'est fait tuer par un crâne de Wither tiré par (un(e)) @2"

msgid "@1 was squashed by a falling anvil"
msgstr "@1 s'est fait écraser par une enclume"

msgid "@1 was squashed by a falling anvil whilst fighting @2"
msgstr "@1 s'est fait écraser par une enclume en combattant @2"

msgid "@1 was squashed by a falling block"
msgstr "@1 a été écrasé(e) par la chute d'un bloc"

msgid "@1 was squashed by a falling block whilst fighting @2"
msgstr "@1 a été écrasé(e) par la chute d'un bloc en combattant @2"

msgid "@1 was slain by @2"
msgstr "@1 s'est fait tuer par un(e) @2"

msgid "@1 was slain by @2 using @3"
msgstr "@1 s'est fait tuer par (un(e)) @2 avec @3"

msgid "@1 was shot by @2"
msgstr "@1 s'est fait tirer dessus par (un(e)) @2"

msgid "@1 was shot by @2 using @3"
msgstr "@1 s'est fait tirer dessus par (un(e)) @2 avec @3"

msgid "@1 was spitballed by @2"
msgstr ""

msgid "@1 was spitballed by @2 using @3"
msgstr ""

msgid "@1 was fireballed by @2"
msgstr "@1 s'est fait tuer par une boule de feu tirée par (un(e)) @2"

msgid "@1 was fireballed by @2 using @3"
msgstr "@1 s'est fait tuer par une boule de feu tirée par (un(e)) @2 avec @3"

msgid "@1 was killed trying to hurt @2"
msgstr "@1 s'est fait tuer en tentant de blesser (un(e)) @2"

msgid "@1 tried to hurt @2 and died by @3"
msgstr "@1 a essayé de blesser @2 et est mort par @3"

msgid "@1 blew up"
msgstr "@1 a péri dans une explosion"

msgid "@1 was blown up by @2"
msgstr "@1 a péri dans une explosion causée par (un(e)) @2"

msgid "@1 was blown up by @2 using @3"
msgstr "@1 a péri dans une explosion causée par (un(e)) @2 avec @3"

msgid "@1 was squished too much"
msgstr "@1 a subi trop de pression"

msgid "@1 was squashed by @2"
msgstr "@1 a été écrasé(e) par @2"

msgid "@1 went off with a bang"
msgstr "@1 est parti(e) avec un bang"

msgid "@1 went off with a bang due to a firework fired by @2 from @3"
msgstr "@1 a explosé à cause d'un feu d'artifice tiré par @2 depuis @3"

msgid "@1 was poked to death by a sweet berry bush"
msgstr "@1 s'est fait piquer à mort par un buisson à baies sucrées"

msgid "@1 was poked to death by a sweet berry bush whilst trying to escape @2"
msgstr "@1 a foncé dans un buisson à baies sucrées en tentant de fuir (un(e)) @2"

